<title>
  <%= title %>
</title>

<div class="d-flex align-items-center justify-content-between">
    <!-- Heading -->
    <h1 class="mb-4">Student List</h1>

    <!-- Add Student Button -->
    <a class="button" href="/student/add_student" btn btn-primary mb-4 style="font-size: 25px;">Add New Student</a>

</div>


<table class ="table table-striped">
    <thead>
      <tr>
        <th scope="col">Sr. No.</th>
        <th scope="col">First Name</th>
        <th scope="col">Last Name</th>
        <th scope="col">Email</th>
        <th scope="col">College</th>
        <th scope="col">Batch</th>
        <th scope="col">DSA Score</th>
        <th scope="col">WebD Score</th>
        <th scope="col">React Score</th>
        <th scope="col">Status</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <% for( let i = 0; i < students.length; i++ ) { %>
          <tr> 
            <th scope="row"><%= i+1 %></th>
            <td><%= students[i]['name'] %></td>
            <td><%= students[i]['last_name'] %></td>
            <td><%= students[i]['email'] %></td>
            <td><%= students[i]['college'] %></td>
            <td><%= students[i]['batch'] %></td>
            <td><%= students[i]['dsa_score'] %></td>
            <td><%= students[i]['webd_score'] %></td>
            <td><%= students[i]['react_score'] %></td>
            <td><%= students[i]['status'] %></td>
          </tr>
        <% } %>
      </tr>
    </tbody>
  </table>
  